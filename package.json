{
  "name": "@europarcel/mcp",
  "version": "1.1.15",
  "description": "Self-hosted MCP server for Europarcel API - Deploy your own instance to enable AI assistants to interact with shipping and logistics services",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "dev:http": "MCP_TRANSPORT=http tsx watch src/index.ts",
    "start": "node dist/index.js",
    "start:http": "MCP_TRANSPORT=http node dist/index.js",
    "test": "jest",
    "lint": "eslint src"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "europarcel",
    "shipping",
    "logistics",
    "api",
    "ai",
    "claude"
  ],
  "author": "Europarcel SRL",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.2",
    "axios": "^1.12.2",
    "dotenv": "^17.2.3",
    "winston": "^3.18.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.6.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "jest": "^29.7.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.45.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/europarcel/mcp-docker.git"
  },
  "bugs": {
    "url": "https://github.com/europarcel/mcp-docker/issues"
  },
  "homepage": "https://github.com/europarcel/mcp-docker#readme"
}
