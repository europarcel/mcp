{
  "name": "@europarcel/mcp-self-hosted",
  "version": "1.1.4",
  "description": "Self-hosted MCP server for Europarcel API - Deploy your own instance to enable AI assistants to interact with shipping and logistics services",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "dev:http": "MCP_TRANSPORT=http tsx watch src/index.ts",
    "start": "node dist/index.js",
    "start:http": "MCP_TRANSPORT=http node dist/index.js",
    "test": "jest",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "europarcel",
    "shipping",
    "logistics",
    "api",
    "ai",
    "claude"
  ],
  "author": "Europarcel SRL",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.0",
    "axios": "^1.7.7",
    "dotenv": "^16.4.7",
    "express-rate-limit": "^8.0.1",
    "winston": "^3.17.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/europarcel/mcp-self-hosted.git"
  },
  "bugs": {
    "url": "https://github.com/europarcel/mcp-self-hosted/issues"
  },
  "homepage": "https://github.com/europarcel/mcp-self-hosted#readme"
}
